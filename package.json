{
  "name": "@mskelton/webpack-config",
  "description": "Shared webpack config for my React projects.",
  "version": "1.0.5",
  "author": "Mark Skelton",
  "repository": {
    "type": "git",
    "url": "https://github.com/mskelton/webpack-config.git"
  },
  "bugs": {
    "url": "https://github.com/mskelton/webpack-config/issues"
  },
  "homepage": "https://github.com/mskelton/webpack-config#readme",
  "license": "MIT",
  "keywords": [
    "webpack",
    "webpack-configuration",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "babel src --out-dir lib --source-maps --extensions .ts",
    "types:generate": "yarn tsc",
    "types:check": "yarn tsc --noEmit --emitDeclarationOnly false",
    "lint": "yarn eslint .",
    "test": "jest",
    "postversion": "git push && git push --tags"
  },
  "eslintConfig": {
    "extends": [
      "@mskelton",
      "@mskelton/eslint-config/typescript"
    ]
  },
  "prettier": "@mskelton/prettier-config",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@mskelton/eslint-config": "^5.10.2",
    "@mskelton/prettier-config": "^5.1.1",
    "@mskelton/tsconfig": "^1.0.2",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/node": "^14.0.11",
    "@types/webpack": "^4.41.17",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "eslint": "^7.2.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "eslint-plugin-sort": "^1.4.0",
    "html-webpack-plugin": "^4.3.0",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0"
  }
}
